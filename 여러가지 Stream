package org.example;

import java.util.stream.IntStream;

class Main {
    public static void main(String[] args) {


        //일반
        for (int i = 1; i <= 10; i++){
            System.out.println(i);
        }
        //스트림
        IntStream.range(1, 11)
                        .forEach(e -> System.out.println(e));
        //스트림
        IntStream.range(1, 11)
                .forEach(System.out::println);
    }
}
